# Description:
#   CDN files for Earth Engine JavaScript API.
#   See https://earthengine.google.com
#
# WARNING:
#   Do not copy from these files or link these files into google3.

load("//tools/build_defs/license:license.bzl", "license")
load("//tools/build_defs/build_test:build_test.bzl", "build_test")

package(
    default_applicable_licenses = ["//third_party/hosted_libraries/libs/earthengine/0.1.346:license"],
    default_visibility = [
        "//third_party/hosted_libraries/libs/earthengine:__pkg__",
    ],
)

license(
    name = "license",
    package_name = "0.1.346",
)

licenses(["notice"])

exports_files(["LICENSE"])

Fileset(
    name = "public",
    out = "public_out",
    entries = [
        FilesetEntry(
            files = glob(
                ["**/*"],
                exclude = [
                    "**/BUILD",
                    "**/METADATA",
                    "**/OWNERS",
                ],
            ),
        ),
    ],
)

build_test(
    name = "public_test",
    targets = [":public"],
)
